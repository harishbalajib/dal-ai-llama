{
	"version": "0.2.0",
	"configurations": [
	  {
		"type": "node",
		"request": "launch",
		"name": "Launch Extension",
		"program": "${workspaceFolder}/extension.js",
		"runtimeExecutable": "${execPath}",
		"args": ["--extensionDevelopmentPath=${workspaceFolder}"],
		"outFiles": ["${workspaceFolder}/**/*.js"],
		"stopOnEntry": false,
		"sourceMaps": true
	  }
	]
  }
  